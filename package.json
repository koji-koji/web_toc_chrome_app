{
  "name": "web_toc_chrome_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export -o extensions/dist",
    "test": "jest --env=jsdom --verbose"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.2.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/react": "17.0.11",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.5",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "jest": "^27.0.5",
    "jest-css-modules": "^2.1.0",
    "postcss": "^8.3.5",
    "typescript": "4.3.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}